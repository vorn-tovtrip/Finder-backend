    "start": "export NODE_ENV=production && tsc --build && node build/index.js ",
    "dev": "export NODE_ENV=development && tsx --watch index.ts",
    "start:dev": "export NODE_ENV=development && nodemon --watch '**/*.ts' --exec 'ts-node' index.ts",
    "shutdown": "tsc --build && node dist/src/utils/script/shutdown.js",
    "postinstall": "prisma generate",
    "prisma:generate:dev": "prisma generate --env-file .env.development",
    "prisma:generate:prod": "prisma generate --env-file .env.production",
    "prisma:migrate:dev": "prisma migrate dev --env-file .env.development",
    "prisma:migrate:prod": "prisma migrate deploy --env-file .env.production"
  },
